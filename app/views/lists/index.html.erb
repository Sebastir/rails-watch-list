<div class="wrapper">

  <div class="banner-index">
    <h1>Welcome to you lists!</h1>
  </div>

  <div class="list-index-content">
  <% @lists.each do |list|  %>
    <%= link_to list_path(list) do %>
      <div class="list-card-index">
      <%= image_tag 'https://media.istockphoto.com/photos/woman-using-her-laptop-for-working-from-home-picture-id1276435469?b=1&k=20&m=1276435469&s=170667a&w=0&h=NtOqX40JegoMjzaxseq4qSz7KhOiWwWimVBI5CvgyO8=', class: 'image-list-cover', alt: 'image cover for list' %>
        <div class="list-card-content">
          <p><%= list.name %></p>
        </div>
      </div>
    <% end  %>
    <% end  %>
  </div>
</div>
